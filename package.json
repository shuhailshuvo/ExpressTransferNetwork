{
  "name": "express-transfer-network",
  "version": "1.0.0",
  "description": "Web Application Frontend for Express Transfer Network",
  "private": true,
  "main": "dist/index.js",
  "homepage": "https://shuhailshuvo.github.io/ExpressTransferNetwork",
  "dependencies": {
    "@date-io/date-fns": "^1.1.0",
    "@date-io/moment": "^1.1.0",
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "classnames": "^2.2.6",
    "gh-pages": "^2.0.1",
    "history": "^4.7.2",
    "material-ui-pickers": "^2.2.4",
    "moment": "^2.24.0",
    "node-sass": "7.0.0",
    "nouislider": "13.1.0",
    "prop-types": "^15.7.1",
    "react": "16.8.1",
    "react-datetime": "2.16.3",
    "react-dom": "16.8.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.5",
    "react-slick": "0.23.2",
    "react-swipeable-views": "0.13.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0"
  },
  "optionalDependencies": {
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-prettier": "3.0.1"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build-package-css": "node-sass src/assets/scss/etn.scss dist/material-kit-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist",
    "compile-sass": "node-sass src/assets/scss/etn.scss src/assets/css/material-kit-react.css",
    "minify-sass": "node-sass src/assets/scss/etn.scss src/assets/css/material-kit-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/etn.scss src/assets/css/material-kit-react.css --source-map true",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
